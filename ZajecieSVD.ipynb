    "import pandas as pd\n",
    "from numpy.linalg import pinv\n",
    "\n",
    "data = pd.read_csv('war3.csv', sep=';')\n",
    "\n",
    "X = data[['x1', 'x2']].values.astype(float)\n",
    "y = data['y'].values.astype(float)\n",
    "\n",
    "a, b = coefficients = pinv(X).dot(y)\n",
    "\n",
    "print(f\"a = {a}\")\n",
    "print(f\"b = {b}\")\n"
